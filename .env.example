# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Google Sheets Configuration
GOOGLE_SHEETS_ID=your_google_sheets_id
GOOGLE_CREDENTIALS={"type": "service_account", "project_id": "your-project-id"}

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_specific_password
EMAIL_FROM=your_email@gmail.com

# Kixie Configuration
KIXIE_API_KEY=your_kixie_api_key

# Priority Scoring Weights
PRIORITY_RECENCY_WEIGHT=0.5
PRIORITY_ENGAGEMENT_WEIGHT=0.3
PRIORITY_CLASS_WEIGHT=0.2
PRIORITY_RECENCY_HALF_LIFE_DAYS=7
PRIORITY_ENGAGEMENT_WINDOW_DAYS=14
PRIORITY_CLASS_SCORES={"hot":1.0,"warm":0.7,"cold":0.3,"default":0.5}

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=your_redis_password
REDIS_SSL=false

# Application Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
